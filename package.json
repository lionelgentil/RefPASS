{
  "name": "soccer-referee-app",
  "version": "1.2.0",
  "description": "Soccer Referee App - Unified Server with Web Interface",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "npm install && mkdir -p data && echo '[]' > data/teams.json && echo '[]' > data/matchdays.json",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "backup": "curl -o backup-$(date +%Y%m%d-%H%M%S).json http://localhost:3000/api/backup"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "soccer",
    "referee",
    "sports",
    "web-app",
    "real-time",
    "sync"
  ],
  "author": "Soccer Referee App",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  }
}